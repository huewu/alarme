<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="vingle_label_text" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/green_ink</item>
        <item name="android:textSize">@dimen/vingle_label_text</item>
    </style>

    <style name="vingle_contet_button" parent="android:Widget.Holo.ImageButton">
        <item name="android:clickable">true</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:paddingLeft">1dp</item>
        <item name="android:paddingRight">1dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
</resources>